[[$ns link $node3 $node6] queue] set limit_ 20

$ns simplex-link $node3 $node8 10.00Mb 20.000000ms $v0
$ns simplex-link-op $node3 $node8 queuePos 0.5
$ns simplex-link-op $node3 $node8 color black
$ns simplex-link-op $node3 $node8 orient 11.6deg
# Set Queue Properties for link 3->8
[[$ns link $node3 $node8] queue] set limit_ 20

$ns simplex-link $node3 $node4 10.00Mb 20.000000ms $v0
$ns simplex-link-op $node3 $node4 queuePos 0.5
$ns simplex-link-op $node3 $node4 color black
$ns simplex-link-op $node3 $node4 orient 74.5deg
