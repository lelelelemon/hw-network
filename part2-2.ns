## node8 at 624.203125,558.718750
$node8 set X_ 624.203125
$node8 set Y_ 558.718750
$node8 set Z_ 0.0
$node8 color "black"

# Create links between nodes.
$ns simplex-link $node8 $node3 10.00Mb 20.000000ms $v0
$ns simplex-link-op $node8 $node3 queuePos 0.5
$ns simplex-link-op $node8 $node3 color black
$ns simplex-link-op $node8 $node3 orient 191.6deg
# Set Queue Properties for link 8->3
[[$ns link $node8 $node3] queue] set limit_ 20

$ns simplex-link $node7 $node2 10.00Mb 20.000000ms $v0
$ns simplex-link-op $node7 $node2 queuePos 0.5
$ns simplex-link-op $node7 $node2 color black
$ns simplex-link-op $node7 $node2 orient 0.6deg
